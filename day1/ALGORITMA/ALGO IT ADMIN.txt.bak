//LOGIN IT ADMIN
MULAI;
SISTEM AJAK USER UNTUK MENGINPUT USERNAME DAN PASSWORD;
USER MENGINPUT DATA KE DALAM FIELD USERNAME DAN PASSWORD;
KLIK BUTTON LOGIN;
SISTEM MENGECEK APAKAH AKUN SUDAH TERMASUK DI LEVEL SUPERADMIN;
JIKA AKUN TIDAK TERMASUK DI LEVEL SUPERADMIN MAKA USER AKAN DIKEMBALIKAN KE FORM LOGIN;
JIKA AKUN SUDAH TERMASUK DI LEVEL SUPERADMIN MAKA USER AKAN DIPINDAHKAN KE DASHBOARD UNTUK SUPERADMIN;
SELESAI;

//DASHBOARD IT ADMIN
MULAI;
USER SUPERADMIN MENDAPAT PRIVILADGE UNTUK MENAMBAH, MENGEDIT, MENGHAPUS DAN MELIHAT DATA(AKUN, DATAGURU, DATASISWA);
USER SUPERADMIN MENDAPAT PRIVILADGE UNTUK GENERATE LAPORAN;
USER SUPERADMIN MENDAPAT PRIVILADGE UNTUK LOGOUT;
SELESAI;

//ALGORITMA MENAMBAH DATA REGISTRASI
MULAI;
USER SUPERADMIN MEMASUKKAN DATA USERNAME DENGAN SQL INSERT KE TABLE AKUN DALAM DATABASE;
USER SUPERADMIN MEMASUKKAN DATA PASSWORD DENGAN SQL INSERT KE TABLE AKUN DALAM DATABASE;
SISTEM AKAN MENYIMPAN DATABASE YANG DIMASUKKAN OLEH ADMIN KE TABLE AKUN;
USER SUPERADMIN TELAH BERHASIL MENAMBAHKAN AKUN BARU;
SELESAI;

//ALGORITMA MENAMBAH DATAGURU
MULAI;
USER SUPERADMIN MENGKLIK BUTTON DATAGURU;
USER SUPERADMIN MENGINPUT DATA GURU DI FIELD YANG DISEDIAKAN;
SISTEM MEMASUKKAN DATA YANG DIINPUT USER SUPERADMIN KE TABLE DATAGURU;
SELESAI;

//ALGORITMA MENGEDIT DATAGURU
MULAI;
USER SUPERADMIN MENGKLIK BUTTON DATAGURU;
USER SUPERADMIN MENGKLIK BUTTON EDIT DI DATA YANG INGIN DI EDIT;
SISTEM MEMINDAHKAN USER SUPERADMIN KE FORM EDIT;
USER MENGEDIT OR MEMBIARKAN DATA YANG DITAMPILKAN DI FIELD TEXT;
USER SUPERADMIN MENGKLIK BUTTON SIMPAN;
SISTEM MENYIMPAN DATA YANG TELAH DIEDIT OLEH USER SUPERADMIN;
SELESAI;

//ALGORITMA MENGHAPUS DATAGURU;
MULAI;
USER SUPERADMIN MENGKLIK BUTTON DATAGURU;
USER SUPERADMIN MENGKLIK BUTTON DELETE DI DATA YANG INGIN DI HAPUS;
SISTEM MENGHAPUS DATA YANG DIPILIH OLEH USER SUPERADMIN;
SELESAI;